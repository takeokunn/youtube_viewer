{
  "name": "youtube_viewer",
  "version": "1.0.0",
  "description": "effective youtube viewer written in javascript(๑╹ω╹๑ )",
  "main": "electron/main.js",
  "scripts": {
    "test": "npm run eslint",
    "eslint": "eslint -c .config/.eslintrc.js src/**/*.js src/**/*.jsx electron/**/*.js",
    "csscomb": "csscomb -c .config/.csscomb.json src/",
    "dev": "npm run dev-webpack & npm run electron-build",
    "dev:web": "npm run dev-webpack & npm run dev-serve",
    "dev-webpack": "webpack -w",
    "dev-serve": "webpack-serve --config webpack.config.js",
    "koa": "node koa/app.js",
    "release": "npm run electron-packager",
    "electron-build": "electron .",
    "electron-packager": "electron-packager . youtube_viewer --platform=darwin --arch=x64 --icon=electron/icon.icns --out=release",
    "deploy": "webpack && gh-pages --dist public --depth 2 --dotfiles true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/takeokunn/youtube_viewer.git"
  },
  "author": "takeokunn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/takeokunn/youtube_viewer/issues"
  },
  "homepage": "https://github.com/takeokunn/youtube_viewer#readme",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "css-loader": "^1.0.1",
    "csscomb": "^4.2.0",
    "dotenv-webpack": "^1.7.0",
    "electron": "^3.1.6",
    "electron-packager": "^12.2.0",
    "eslint": "^5.15.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^2.0.0",
    "gh-pages": "^2.0.1",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "serviceworker-webpack-plugin": "^1.0.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-serve": "^2.0.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "axios": "^0.18.0",
    "koa": "^2.7.0",
    "koa-static": "^5.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^5.1.1",
    "react-redux-form": "^1.16.12",
    "react-redux-toastr": "^7.4.8",
    "react-string-replace": "^0.4.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.2",
    "redux-saga-devtools": "^0.3.0"
  }
}
